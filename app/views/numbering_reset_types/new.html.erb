<h1>New numbering_reset_type</h1>

<% form_for(@numbering_reset_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :script %><br />
    <%= f.text_area :script %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', numbering_reset_types_path %>
